	<script type="text/javascript"> // Unsure if I need to include script type for this?


// testOne
// Each array contains data corresponding to one property of a fortune cookie

	// 'FORTUNES'
		
		// Array containing data for 'fortunes' 

			var fortune = ['Your life is a dashing and bold adventure', 'Many successes will accompany you this year', 'Good luck is the result of good planning'];

		// Function to select and return random fortune

			var newFortune = function () {
			var todaysFortune = fortune[Math.floor(Math.random() fortune.length)];
			return todaysFortune;
			};

	// 'LEARN CHINESE'

		// Array containing data for 'learnChinese'

			var learnChinese = ['shēng cài 生菜' + ' ' + 'Lettuce', 'yòu zi 柚子' + ' ' + 'Grapefruit', 'qié zi 茄子' + ' ' + 'Eggplant'];

		// Function to select and return random 'learnChinese'

			var newLearnChinese = function () {
			var todaysLearnChinese = learnChinese[Math.floor(Math.random() learnChinese.length)];
			return todaysLearnChinese;
			};

// 'LUCKY NUMBERS'

	// Array containing data for 'luckyNumbers'

		var luckyNumbers = ['18, 16, 43, 41, 35, 36', '2, 39, 5, 38, 16, 29', '30, 1, 46, 14, 44, 26'];

	// Function to select and return random 'luckyNumbers'

		var newLuckyNumbers = function () {
		var todaysLuckyNumbers = luckyNumbers[Math.floor(Math.random() luckyNumbers.length)];
		return todaysLuckyNumbers;
		};

		console.log(todaysFortune);
		console.log(todaysLearnChinese);
		console.log(todaysLuckyNumbers);

</script>
